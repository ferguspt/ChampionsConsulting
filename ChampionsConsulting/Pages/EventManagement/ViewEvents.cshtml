    <h2>All Events</h2>
    <div class="container-table100">
        <div class="wrap-table100">
            <div class="table100 ver6 m-b-110">
                <form method="post">
                    <table data-vertable="ver6">
                        <thead>
                            <tr class="row100 head">
                                <th class="column100 column1" data-column="column1">Name:</th>
                                <th class="column100 column2" data-column="column2">Description:</th>
                                <th class="column100 column3" data-column="column3">Start Date And Time:</th>
                                <th class="column100 column4" data-column="column4">End Date And Time:</th>
@*                                 <th class="column100 column5" data-column="column5">Edit Event</th>
                                <th class="column100 column6" data-column="column6">Delete Event</th> *@
                            </tr>
                        </thead>

                        <tbody>
                            @{
                                foreach (var Event in Model.EventList)
                                {
                                    <tr class="row100">
                                        @*<td class="invisible">@Event.EventID</td>*@
                                        <td class="column100 column1" data-column="column1"><a asp-page="/EventManagement/SubEventInfo">@Event.Name</a></td>
                                        <td class="column100 column2" data-column="column2">@Event.Description</td>
                                        <td class="column100 column3" data-column="column3">@Event.StartDateAndTime</td>
                                        <td class="column100 column4" data-column="column4">@Event.EndDateAndTime</td>
                                        @*@if (HttpContext.Session.GetString("UserType") = "Organizer" || "Admin" {

                                        } else
                                        {
                                            
                                        }*@
                                        @* <td class="column100 column5" data-column="column5">
                                             <a asp-page="/EventManagement/Edit" class="btn btn-primary" asp-route-eventid="@Event.EventID">Edit</a>
                                         </td>

                                         <td class="column100 column6" data-column="column6">
                                             <a asp-page="/EventManagement/Delete" class="btn btn-danger" asp-route-eventid="@Event.EventID">Delete</a>
                                         </td>*@
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </form>
            </div>
        </div>
    </div>


